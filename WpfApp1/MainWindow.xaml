<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        xmlns:pages="clr-namespace:WpfApp1.Pages"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl TabStripPlacement="Top">
            <TabItem Name="diceRollerTab">
                <TabItem.Header>Dice Roller</TabItem.Header>
                <TabItem.Content>
                    <Frame Source="Pages/DiceRollerTab.xaml" />
                </TabItem.Content>
            </TabItem>
            <TabItem Name="clocksTab">
                <TabItem.Header>Clocks</TabItem.Header>
            </TabItem>
            <TabItem Name="statTrackerTab">
                <TabItem.Header>Stat Tracker</TabItem.Header>
            </TabItem>
            <TabItem Name="memosTab">
                <TabItem.Header>Memos</TabItem.Header>
            </TabItem>
            <TabItem Name="sessionLogsTab">
                <TabItem.Header>Session Logs</TabItem.Header>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
